@model IEnumerable<IS356Main.Models.Presentations>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Presenters)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pLink)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pPicture)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pTitle)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Presenters)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pPicture)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pTitle)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

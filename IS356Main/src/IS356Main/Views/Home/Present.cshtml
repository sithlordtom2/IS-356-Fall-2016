@model IEnumerable<IS356Main.Models.Presentations>  

@{
    ViewData["Title"] = "Presentations";
}
<h2>@ViewData["Title"]</h2>

@foreach(var item in Model)
{
    <div class="col-md-6">
        <h3><a href=@Url.Content(item.pLink) download>@Html.DisplayFor(Model => item.pTitle)</a></h3><br />
        <h4>@Html.DisplayFor(Model => item.Presenters)</h4> 
    </div>
}
<div class="row"></div>
<br />
